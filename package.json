{
  "name": "zbox-challenge",
  "version": "0.0.1",
  "description": "Desafio de zbox para evaluar habilidades",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-linux": "electron-packager . zbox-challenge --platform=linux --arch=x64 --version=0.31.1 --out=dist --ignore=\"node_modules/(electron-packager|electron-prebuilt|asar)\" --asar",
    "build-darwin": "electron-packager . zbox-challenge --platform=darwin --arch=x64 --version=0.31.1 --out=dist --ignore=\"node_modules/(electron-packager|electron-prebuilt|asar)\" --asar",
    "build-win32": "electron-packager . zbox-challenge --platform=win32 --arch=x64 --version=0.31.1 --out=dist --ignore=\"node_modules/(electron-packager|electron-prebuilt|asar)\" --asar"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/friverosicc/zbox-challenge.git"
  },
  "author": "Francisco Riveros",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/friverosicc/zbox-challenge/issues"
  },
  "homepage": "https://github.com/friverosicc/zbox-challenge#readme",
  "dependencies": {
    "angular": "^1.4.5",
    "angular-animate": "^1.4.5",
    "angular-aria": "^1.4.5",
    "angular-material": "^0.10.1",
    "electron-prebuilt": "^0.31.1"
  },
  "devDependencies": {
    "asar": "^0.8.0",
    "electron-packager": "^5.0.2"
  }
}
